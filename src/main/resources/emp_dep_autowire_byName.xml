<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context.xsd">

	<bean id="employee1" class="com.livetechstudy.spring.autowire.Employee" autowire="byName">
		<property name="name" value="First Employee"/>
	</bean>
	
	<bean id="department" class="com.livetechstudy.spring.autowire.Department">
		<property name="departmentID" value="100"/>
		<property name="detpartmentName" value="Development"></property>
	</bean>
</beans>